# Configuração da API Externa
# Copie este arquivo para .env e configure suas variáveis

# URL base da API externa
VITE_API_BASE_URL=https://api.exemplo.com

# Credenciais da API (opcional - pode ser configurado via interface)
VITE_API_USERNAME=seu_usuario
VITE_API_PASSWORD=sua_senha

# Chave da API (se necessário)
VITE_API_KEY=sua_chave_api

# Configurações adicionais
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3

# Modo de desenvolvimento
VITE_DEV_MODE=true
